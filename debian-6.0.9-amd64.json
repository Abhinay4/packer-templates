{
  "builders": [{
    "type": "virtualbox-iso",
    "guest_os_type": "Debian_64",
    "iso_url": "http://cdimage.debian.org/cdimage/archive/6.0.9/amd64/iso-cd/debian-6.0.9-amd64-netinst.iso",
    "iso_checksum": "6d66c90c0d554801f537a794c4b778ef549efd9d4c7d68b893846c647f257e21",
    "iso_checksum_type": "sha256",
    "output_directory": "output-debian-6.0.9-amd64",
    "vm_name": "packer-debian-6.0.9-amd64",
    "disk_size": "{{user `disk_size`}}",
    "headless": "{{user `headless`}}",
    "http_directory": "http",
    "boot_wait": "5s",
    "boot_command": [
      "<esc><wait>",
      "auto ",
      "preseed/url=http://{{.HTTPIP}}:{{.HTTPPort}}/debian-6.0.9/preseed.cfg ",
      "<enter>"
    ],
    "ssh_wait_timeout": "60m",
    "ssh_username": "vagrant",
    "ssh_password": "vagrant",
    "shutdown_command": "sudo poweroff",
    "vboxmanage": [
      ["modifyvm", "{{.Name}}", "--memory", "512"],
      ["modifyvm", "{{.Name}}", "--cpus", "1"]
    ]
  }],
  "provisioners": [{
    "type": "shell",
    "scripts": [
      "scripts/debian/base.sh",
      "scripts/debian/virtualbox.sh",
      "scripts/common/vagrant.sh",
      "scripts/debian/cleanup.sh",
      "scripts/common/minimize.sh"
    ]
  }],
  "post-processors": [{
    "type": "vagrant",
    "compression_level": "{{user `compression_level`}}",
    "output": "debian-6.0.9-amd64.box"
  }],
  "variables": {
    "compression_level": 1,
    "disk_size": 40000,
    "headless": false
  }
}
